    "vuex-class": "^0.3.2",
    "webpack-cli": "^3.3.11"
  },
  "engines": {
    "node": ">= 8.15.0",
    "npm": ">= 5.6.0"
  },
  "pre-commit": [
    "lint"
  ],
  "pre-push": []
}