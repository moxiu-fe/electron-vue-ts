    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^6.2.2",
    "native-ext-loader": "^2.3.0",
    "node-loader": "^0.6.0",
    "node-pre-gyp": "^0.13.0",
    "node-sass": "^4.13.1",
    "postcss-reduce-idents": "^4.0.2",
    "pre-commit": "^1.2.2",
    "pre-push": "^0.1.1",
    "pumpify": "^2.0.1",
    "sass-loader": "^8.0.2",
    "spectron": "^3.8.0",
    "split2": "^3.1.1",
    "terser-webpack-plugin": "^2.3.5",
    "through2-filter": "^3.0.0",
    "through2-map": "^3.0.0",
    "typescript": "^3.8.3",
